{
  "version": 2,
  "projects": {
    "db": "apps/db",
    "functions": "apps/functions",
    "storage": "apps/storage",
    "www": "apps/www",
    "www-e2e": "apps/www-e2e"
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "www",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  }
}
